{
  "env": {
    "browser": true,
    "es2021": true,
    "jest": true
  },
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "no-unused-vars": "warn",
    "no-undef": "warn",
    "no-console": "off",
    "eqeqeq": "error",
    "no-var": "warn",
    "prefer-const": "warn",
    "no-duplicate-case": "error",
    "no-empty": "warn",
    "no-unreachable": "error",
    "curly": "warn",
    "no-eval": "error"
  }
}
